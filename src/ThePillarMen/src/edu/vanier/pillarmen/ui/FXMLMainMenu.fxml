<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../stylesheets/LauncherStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.vanier.pillarmen.controllers.FXMLMainMenuController">
   <children>
      <Label layoutX="88.0" layoutY="108.0" text="Welcome to the Region Explorer Application!" textFill="#3b9400">
         <font>
            <Font name="Sitka Text Bold Italic" size="22.0" />
         </font>
      </Label>
      <Button fx:id="btnLaunch" contentDisplay="TOP" layoutX="269.0" layoutY="181.0" mnemonicParsing="false" onAction="#launchApplication" style="-fx-background-color: SEAGREEN;" text="Launch" textOverrun="CENTER_WORD_ELLIPSIS">
         <font>
            <Font name="AR CENA" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnClose" layoutX="270.0" layoutY="280.0" mnemonicParsing="false" onAction="#closeMenu" prefHeight="41.0" prefWidth="77.0" style="-fx-background-color: CRIMSON;" text="Close">
         <font>
            <Font name="AR CENA" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
